{
  "name": "vuepoint",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn run serve",
    "bootstrap": "lerna bootstrap",
    "serve": "yarn storybook:serve",
    "build": "lerna run build",
    "lint": "vue-cli-service lint",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook -s ./public -o ./static/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook -s ./public",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "bulma": "^0.7.5",
    "core-js": "^3.2.1",
    "vue": "^2.6.10",
    "vuex": "^3.1.1",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.2.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addon-notes": "^5.2.1",
    "@storybook/addon-storysource": "^5.2.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.11",
    "@types/react": "^16.9.5",
    "@types/storybook__react": "^4.0.2",
    "@vue/cli-plugin-babel": "^4.0.0-rc.7",
    "@vue/cli-plugin-typescript": "^4.0.0-rc.7",
    "@vue/cli-plugin-unit-jest": "^4.0.0-rc.7",
    "@vue/cli-service": "^4.0.0-rc.7",
    "@vue/test-utils": "^1.0.0-beta.29",
    "lerna": "^3.16.4",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.3.1",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "vue-cli-plugin-storybook": "^0.6.1",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "workspaces": [
    "packages/*"
  ]
}
