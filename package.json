{
  "name": "vuepoint",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "serve": "yarn storybook:serve",
    "build": "lerna run build",
    "lint": "vue-cli-service lint",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook -s ./public -o ./static/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook -s ./public",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "bulma": "^0.7.5",
    "core-js": "^3.1.4",
    "vue": "^2.6.10",
    "vuex": "^3.1.1",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.1.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.1.8",
    "@storybook/addon-knobs": "^5.1.8",
    "@storybook/addon-links": "^5.1.8",
    "@storybook/addon-notes": "^5.1.8",
    "@storybook/addon-storysource": "^5.1.8",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.20",
    "@types/storybook__react": "^4.0.2",
    "@vue/cli-plugin-babel": "^4.0.0-alpha.0",
    "@vue/cli-plugin-typescript": "^4.0.0-alpha.0",
    "@vue/cli-plugin-unit-jest": "^4.0.0-alpha.0",
    "@vue/cli-service": "^4.0.0-alpha.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "lerna": "^3.15.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2",
    "vue-cli-plugin-storybook": "^0.6.1",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "workspaces": [
    "packages/*"
  ]
}
